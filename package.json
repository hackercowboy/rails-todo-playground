{
  "name": "rails-todo-playground",
  "version": "1.0.0",
  "description": "My little rails playground",
  "main": "index.js",
  "repository": "git@github.com:hackercowboy/rails-todo-playground.git",
  "author": "david@uebelacker.ch",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js app/javascript",
    "lint:fix": "eslint --fix --ext .js app/javascript",
    "test:coverage": "jest --coverage",
    "test": "jest --coverage --watch",
    "start": "bin/dev",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx",
    "watch": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx --watch"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "esbuild": "^0.14.42",
    "formik": "^2.2.9",
    "jest-environment-jsdom": "^28.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-htm": "^0.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "jest": "^28.1.0",
    "lcov-result-merger": "^3.2.0"
  }
}
