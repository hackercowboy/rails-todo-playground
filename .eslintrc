{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "plugins": [
        "htm"
    ],
    "rules": {
        "import/no-unresolved": "off"
    },
    "env": {
        "browser": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}